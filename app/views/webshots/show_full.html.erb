<fieldset >
<legend>
	<%= link_to 'Back', :back , :class => "btn btn-primary"%>
	<%= link_to 'Share', user_signed_in? && current_user.profile ? "#share-by-email-#{@webshot.id}" : "#please-login-profile", :class => "btn btn-warning", :data => { toggle: "modal"}%>





<%= @webshot.title if @webshot.title %>

</legend>


<div class="text-center">
<%= image_tag(@webshot.photo.url(:original), 
                :alt => @webshot.url,
                :title => "Link to full-size image in a new window.",
                :class => "col-12"
                 )%>



</div>


</fieldset>


<%= render 'partial/share_by_email', w: @webshot %>